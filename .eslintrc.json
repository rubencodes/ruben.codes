{
  "env": {
    "browser": true,
    "node": true
  },
  "extends": ["plugin:@typescript-eslint/recommended", "plugin:react/recommended"],
  "plugins": [
    "@typescript-eslint"
  ],
  "parser": "@typescript-eslint/parser",
  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true,
      "impliedStrict": true
    }
  },
  "globals": {
    "Set": true,
    "Map": true,
    "AWS": true,
    "Promise": true
  }
}
